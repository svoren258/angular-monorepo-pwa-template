{
  "name": "platform-feature",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/platform-feature/src",
  "prefix": "angular-monorepo-pwa-template",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/platform-feature/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/platform-feature/**/*.ts",
          "libs/platform-feature/**/*.html"
        ]
      }
    }
  }
}
